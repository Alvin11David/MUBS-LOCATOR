{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "/firebase-messaging-sw.js",
        "headers": [
          {
            "key": "Content-Type",
            "value": "text/javascript"
          }
        ]
      }
    ],
    "rewrites": [
      {
        "source": "/directions",
        "function": "directions"
      }
    ]
  },
  "functions": {
    "source": "functions"
  }
}